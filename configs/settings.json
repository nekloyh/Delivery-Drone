{
  "SettingsVersion": 1.2,
  "SimMode": "Multirotor",
  "ClockSpeed": 3.0,
  
  "ViewMode": "FlyWithMe",
  "EnableCollisionPassthrough": true,
  "EnableRpcSceneObjects": true,
  "RpcEnabled": true,
  "RpcPort": 41451,
  "LocalHostIp": "127.0.0.1",
  
  
  "EngineSettings": {
    "DisableRendering": false,
    "EnableCollision": true,
    "EnablePhysics": true,
    "EnableWind": false
  },
  
  
  "Vehicles": {
    "Drone1": {
      "VehicleType": "SimpleFlight",
      "AutoCreate": true,
      "DefaultVehicleState": "Disarmed",
      
      "Cameras": {
        "front_center": {
          "CaptureSettings": [
            {
              "ImageType": 0,
              "Width": 320,
              "Height": 240,
              "FOV_Degrees": 90
            }
          ],
          "X": 0.1,
          "Y": 0.0,
          "Z": 0.0
        }
      },
      
      "Sensors": {
        "Barometer":    { "SensorType": 1, "Enabled": true  },
        "Imu":          { "SensorType": 2, "Enabled": true,  "UpdateFrequency": 200 },
        "Gps":          { "SensorType": 3, "Enabled": false },
        "Magnetometer": { "SensorType": 4, "Enabled": true  },
        "DistanceFront":{ "SensorType": 5, "Enabled": true  },        
        "LidarFront": {
          "SensorType": 6,
          "Enabled": true,
          "NumberOfChannels": 16,
          "PointsPerSecond": 30000,
          "Range": 50,
          "RotationsPerSecond": 10
        }
      }
    }
  }
}